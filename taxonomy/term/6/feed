    &lt;div class=&quot;field-items&quot;&gt;
          &lt;div class=&quot;field-item even&quot;&gt;&lt;a href=&quot;/tags/sustainable&quot;&gt;Sustainable&lt;/a&gt;&lt;/div&gt;
          &lt;div class=&quot;field-item odd&quot;&gt;&lt;a href=&quot;/tags/green&quot;&gt;Green&lt;/a&gt;&lt;/div&gt;
          &lt;div class=&quot;field-item even&quot;&gt;&lt;a href=&quot;/tags/wild-flowers&quot;&gt;wild flowers&lt;/a&gt;&lt;/div&gt;
          &lt;div class=&quot;field-item odd&quot;&gt;&lt;a href=&quot;/tags/seed-bombs&quot;&gt;seed bombs&lt;/a&gt;&lt;/div&gt;
          &lt;div class=&quot;field-item even&quot;&gt;&lt;a href=&quot;/tags/anthropology&quot;&gt;anthropology&lt;/a&gt;&lt;/div&gt;
          &lt;div class=&quot;field-item odd&quot;&gt;&lt;a href=&quot;/tags/uncommon-goods&quot;&gt;uncommon goods&lt;/a&gt;&lt;/div&gt;
